{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testRegex": "(/__tests__/.*|(\\.|/)(spec|e2e-spec))\\.ts$",
  "testMatch": ["**/?(*.)+(spec|test).[jt]s?(x)", "**/test/**/*.ts"],
  "testPathIgnorePatterns": ["/node_modules/"],
  "coverageDirectory": "./coverage",
  "testEnvironment": "node"
}
